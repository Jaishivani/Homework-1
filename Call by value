//call by value
//argument passed and no return
#include<stdio.h>
void main()
{
int a;
printf("Enter number:");
scanf("%d",&a);
printf("Given number is %d\n",a);
inc(a);//Actual parameter
printf("After incrementing %d\n",a);
}
void inc(int a)//Formal parameter
{
a=a+1;
printf("Increment is  %d\n",a);
}
